{{#if this.isloggedin}}
<html lang="en">

<head>
    {{app-header}}
<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
</head>

<body>
    <div class="app-container app-theme-white body-tabs-shadow fixed-sidebar fixed-header">
        {{top-bar}}
        <div class="app-main">
            {{nav-bar}}
            <div class="app-main__outer">
                <div class="app-main__inner">
                    {{page-header}}
                    <div class="row">
                        <div class="col-md-6 col-xl-4">
                            {{header-card bgcolor="midnight-bloom" textcolor="white" heading="Hello world"
                            subheading="Hey there" value="1205"}}
                        </div>
                        <div class="col-md-6 col-xl-4">
                            {{header-card bgcolor="arielle-smile" textcolor="white" heading="Clients" subheading="Total
                            Clients Profit" value="1896"}}
                        </div>
                        <div class="col-md-6 col-xl-4">
                            {{header-card bgcolor="grow-early" textcolor="white" heading="Followers" subheading="People
                            Interested" value="23%"}}
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6 col-xl-4">
                            {{header-card heading="Total Orders" subheading="Last year expenses" numcolor="success"
                            value="1896"}}
                        </div>
                        <div class="col-md-6 col-xl-4">
                            {{header-card heading="Products Sold" subheading="Revenue streams" numcolor="warning"
                            value="$3M"}}
                        </div>
                        <div class="col-md-6 col-xl-4">
                            {{header-card heading="Followers" subheading="People Interested" numcolor="danger"
                            value="45.9%"}}
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 col-lg-6">
                            {{mf-chart}}
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            {{mf-table }}
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6 col-lg-3">
                            {{progress-bar color="danger" text="Income Target" percentage="71"}}
                        </div>
                        <div class="col-md-6 col-lg-3">
                            {{progress-bar color="success" text="Expenses Target" percentage="54"}}
                        </div>
                        <div class="col-md-6 col-lg-3">
                            {{progress-bar color="warning" text="Spendings Target" percentage="32"}}
                        </div>
                        <div class="col-md-6 col-lg-3">
                            {{progress-bar color="info" text="hello eorld" percentage="89"}}
                        </div>
                    </div>
                </div>
                {{app-footer}}
            </div>
            <script src="http://maps.google.com/maps/api/js?sensor=true"></script>
        </div>
    </div>
    <script type="text/javascript" src="./assets/scripts/main.js"></script>
</body>

</html>

{{else}}
{{login}}
{{/if}}